@article{DICOSMO20442022,
  title = {204.4 {{Identifiers}} for {{Digital Objects}}: {{The}} Case of Software Source Code Preservation.},
  shorttitle = {204.4 {{Identifiers}} for {{Digital Objects}}},
  author = {Di Cosmo, Roberto and Gruenpeter, Morane and Zacchiroli, Stefano},
  namea = {Potterbusch, Megan and McGovern, Nancy and {Open Science Framework}},
